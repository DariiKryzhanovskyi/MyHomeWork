using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace MyHomeWork
{
    internal class Program
    {
        static void Main(string[] args)
        {
            int totalPictures = 52;
            int picturesInRow = 3;

            int filledRows = totalPictures / picturesInRow;
            int picturesRemaining = totalPictures % picturesInRow;

            Console.WriteLine($"Заполненных рядов: {filledRows}");
            Console.WriteLine($"Картинки oставшийся: {picturesRemaining}");

            Console.ReadKey();
        }
    }
}
